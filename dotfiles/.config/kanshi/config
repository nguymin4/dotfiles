profile docked-dell-latitude {
	output eDP-1 disable
	output HDMI-A-1 enable
}

profile mobile-dell-latitude {
	output eDP-1 enable
}

profile docked-dell-precision-intel {
	output eDP-1 disable
	output DP-3 enable
}

profile mobile-dell-precision-intel {
	output eDP-1 enable
}

profile docked-dell-precision-nvidia {
	output eDP-1-1 disable
	output DP-1-3 enable
}

profile mobile-dell-precision-nvidia {
	output eDP-1-1 enable
}
