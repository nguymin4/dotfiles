{
  "coc.preferences.useQuickfixForLocations": true,
  "diagnostic.enableMessage": "jump",
  "diagnostic.errorSign": "E",
  "diagnostic.hintSign": "H",
  "diagnostic.infoSign": "I",
  "diagnostic.virtualText": false,
  "diagnostic.warningSign": "W",
  "explorer.width": 40,
  "explorer.previewAction.onHover": false,
  "explorer.sources": [
    { "name": "file", "expand": true }
  ],
  "explorer.buffer.child.template": "[selection | 1] [bufnr] [name] [modified][readonly]",
  "explorer.file.reveal.auto": false,
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1]",
  "explorer.file.column.indent.indentLine": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "vim-devicons",
  "explorer.icon.selected": "",
  "explorer.icon.customIcons": {
    "icons": {
      "file": {
        "code": "",
        "color": "#e5c07b"
      }
    }
  },
  "explorer.git.icon.status.added": "✚",
  "explorer.git.icon.status.deleted": "✖",
  "explorer.git.icon.status.ignored": "ⁱ",
  "explorer.git.icon.status.renamed": "≫",
  "explorer.git.icon.status.modified": "•",
  "explorer.git.icon.status.untracked": "ᵁ",
  "explorer.file.column.clip.copy": "c",
  "explorer.file.column.clip.cut": "x",
  "explorer.openAction.strategy": "previousWindow",
  "explorer.keyMappingMode": "none",
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "i": "open:split:intelligent",
    "s": "open:vsplit",
    "cd": "cd",
    "u": "gotoParent",
    "R": "refresh",
    "gs": "reveal:select",
    "ma": "addFile",
    "mA": "addDirectory",
    "mr": "rename",
    "dd": "cutFile",
    "yy": "copyFile",
    "yp": "copyFilepath",
    "yn": "copyFilename",
    "p": "pasteFile",
    "D": "delete",
    "f": "search",
    "F": "searchRecursive",
    "X": "collapse",
    "gk": "expandablePrev",
    "gj": "expandableNext",
    "I": "toggleHidden",
    "<space>": "toggleSelection",
    "J": ["toggleSelection", "nodeNext"],
    "K": ["toggleSelection", "nodePrev"],
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",
    "[[": "sourcePrev",
    "]]": "sourceNext",
    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",
    "[c": "gitPrev",
    "]c": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage",
    "?": "help",
    "q": "quit",
    "<esc>": "esc"
  },
  "explorer.trash.command": "trash-put %l",
  "floatFactory.floatConfig": {
    "border": true,
    "rounded": true,
  },
  "hover.previewMaxHeight": 24,
  "inlayHint.enable": false,
  "suggest.completionItemKindLabels": {
    "class" : "󰠱",
    "color" : "󰏘",
    "constant" : "󰏿",
    "constructor" : "",
    "default": "",
    "enum": "",
    "enumMember": "",
    "event" : "",
    "field" : "󰜢",
    "file" : "󰈙",
    "folder" : "󰉋",
    "function" : "󰊕",
    "interface" : "",
    "keyword" : "󰌋",
    "method" : "󰆧",
    "module" : "",
    "operator" : "󰆕",
    "property" : "󰜢",
    "reference" : "󰈇",
    "snippet" : "",
    "struct" : "󰙅",
    "text" : "󰉿",
    "typeParameter" : "",
    "unit" : "󰑭",
    "value" : "󰎠",
    "variable" : "󰀫",
  },
  "suggest.floatConfig": {
    "border": true,
    "rounded": true,
  },
  "suggest.minTriggerInputLength": 1,
  "suggest.noselect": true,
  "suggest.timeout": 5000,
  "suggest.virtualText": false,
}
